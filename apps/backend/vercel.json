{
  "version": 2,
  "installCommand": "corepack enable && corepack yarn install --immutable",
  "buildCommand": "yarn db:generate && yarn build",
  "functions": {
    "api/[...all].js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/$1"
    }
  ],
  "outputDirectory": "dist"
}
