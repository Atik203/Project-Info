{
  "version": 2,
  "installCommand": "corepack enable && yarn install --immutable",
  "buildCommand": "yarn db:generate && yarn build",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
